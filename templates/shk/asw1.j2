no service pad
service tcp-keepalives-in
service tcp-keepalives-out
service timestamps debug datetime msec localtime
service timestamps log datetime msec localtime
service password-encryption
!
aaa new-model
aaa local authentication attempts max-fail 5
aaa authentication login default local
aaa authorization exec default local
!
hostname SHK-{{ data["telephony_prefix"] }}-ASW1
ip domain-name {{ data["domain_name"] }}
username {{ data["creds"]["username"] }} privilege 15 secret {{ data["creds"]["password"] }}
enable secret {{ data["creds"]["password"] }}
ip dhcp snooping
ip dhcp snooping vlan 10,20,30,50,90
no ip dhcp snooping information option
no ip routing
!
spanning-tree mode rapid-pvst
vtp mode off
vlan 10
 name DATA
vlan 20
 name BIOMED
vlan 30
 name VOICE
vlan 40
 name MGMT
vlan 50
 name SPECIAL
vlan 90
 name TRUNK_AP
!
ip ssh version 2
ip scp server enable
crypto key generate rsa modulus 2048
!
{% for i in range(44) -%}
interface GigabitEthernet1/0/{{ i+1 }}
 description USER/VOICE
 switchport mode access
 switchport access vlan 10
 switchport voice vlan 30
 switchport port-security
 switchport port-security violation restrict
 switchport port-security maximum 2
 spanning-tree portfast
 no shutdown
!
{% endfor %}
{% for i in range(44, 48) -%}
interface GigabitEthernet1/0/{{ i+1 }}
 description VLAN TRUNK
 switchport mode trunk
 switchport trunk native vlan 90
 switchport nonegotiate
 ip dhcp snooping trust
 no shutdown
!
{% endfor %}
interface Vlan40
 description MGMT VLAN
 ip address {{ data["addr"]["asw1"]["mgmt_svi40"] }} {{ data["subnets"]["mgmt"].netmask }}
 no shutdown
!
ip default-gateway {{ data["addr"]["dsw"]["mgmt_svi40"] }}
banner motd %
